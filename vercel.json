{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "^/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "^/$",
      "dest": "/public/index.html"
    },
    {
      "src": "^/(register|auth)/?$",
      "dest": "/dist/index.html"
    },
    {
      "src": "^/assets/index-.*\\.js$",
      "dest": "/assets/index-DErrNw1j.js"
    },
    {
      "src": "^/assets/index-.*\\.css$",
      "dest": "/assets/index-CLgawkCm.css"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}