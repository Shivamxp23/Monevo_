{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
  { "src": "/", "dest": "/index.html" },
  { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}
