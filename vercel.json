{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "^/register/?$",
      "dest": "dist/index.html",
      "status": 200
    },
    {
      "src": "^/auth/?$",
      "dest": "dist/index.html",
      "status": 200
    },
    {
      "src": "^/assets/(.*)$",
      "dest": "/assets/$1"
    },
    {
      "src": "^/css/(.*)$",
      "dest": "/css/$1"
    },
    {
      "src": "^/$",
      "dest": "index.html"
    },
    {
      "src": "^/dist/(.*)$",
      "dest": "dist/$1"
    },
    {
      "src": "^/(.+)$",
      "dest": "dist/index.html"
    }
  ]
}